{
  "name": "tcldcamexpo",
  "version": "1.0.15",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "NODE_ENV=production npx expo export --platform web",
    "serve": "npx serve dist -l 8080 -s --cors",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "deploy:patch": "npm run version:patch && npm run build && npm run deploy:push",
    "deploy:minor": "npm run version:minor && npm run build && npm run deploy:push",
    "deploy:major": "npm run version:major && npm run build && npm run deploy:push",
    "deploy:push": "git add . && git commit -m \"ðŸš€ Deploy v$(node -p \"require('./package.json').version\") - $(date '+%Y-%m-%d %H:%M')\" && git push origin main"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.5",
    "expo": "~53.0.20",
    "expo-av": "~15.1.7",
    "expo-camera": "~16.1.11",
    "expo-file-system": "~18.1.11",
    "expo-media-library": "~17.1.7",
    "expo-modules-core": "^2.5.0",
    "expo-status-bar": "~2.2.3",
    "express": "^4.21.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "serve": "^14.2.4",
    "typescript": "~5.8.3"
  },
  "private": true
}
